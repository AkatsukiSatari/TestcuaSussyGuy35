        "import math",

        "    def tonguocnguyento(n):\n",
        "        if n <= 0:\n",
        "            return \"khong phai so duong"
           
        "        tong = 0\n",
           
        "       if n % 2 == 0:\n",
        "            tong += 2\n",
        "            while n % 2 == 0:\n",
        "                n //= 2\n",
                    
        "        for i in range(3, int(math.sqrt(n)) + 1, 2):\n",
        "            if n % i == 0:\n",
        "                tong += i\n",
        "                while n % i == 0:\n",
        "                    n //= i\n",
                       
         "       if n > 1:\n",
         "           tong += n",   
         "       return tong\n",

         "n = int(input("n thuoc N, n = ")) \n",
         "A = tonguocnguyento(n)\n",
         "print(f"Tong uoc nguyen to: {A}")\n",
    "\n",
