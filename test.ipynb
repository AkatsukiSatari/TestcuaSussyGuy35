   "source": [
        "import math",

        "    def tonguocnguyento(n):",
        "        if n <= 0:",
        "            return ("khong phai so duong") ",
           
        "        tong = 0"
           
        "       if n % 2 == 0:",
        "            tong += 2",
        "            while n % 2 == 0:",
        "                n //= 2",
                    
        "        for i in range(3, int(math.sqrt(n)) + 1, 2): ",
        "            if n % i == 0:",
        "                tong += i",
        "                while n % i == 0:",
        "                    n //= i",
                       
         "       if n > 1:",
         "           tong += n",   
         "       return tong",

         "n = int(input("n thuoc N, n = ")) ",
         "A = tonguocnguyento(n)",
         "print(f"Tong uoc nguyen to: {A}") ",
    "\n",
